{
  "name": "mi-proyecto-nodejs",
  "version": "1.0.0",
  "description": "Proyecto de aprendizaje Node.js con arquitectura de 3 capas",
  "main": "server.js",
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\"",
    "client": "live-server public",
    "build": "echo 'Build complete'",
    "vercel-build": "echo 'Vercel build complete'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js"
  },
  "keywords": [
    "nodejs",
    "express",
    "mvc",
    "api",
    "vercel"
  ],
  "author": "Diana Zuniga",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.2",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2"
  }
}
